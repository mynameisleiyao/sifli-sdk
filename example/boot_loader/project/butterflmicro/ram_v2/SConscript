import os
from building import *

cwd = GetCurrentDir()
objs = []
list = os.listdir(cwd)

# Add board
objs.extend(SConscript(os.path.join(cwd, '../board/SConscript'), variant_dir="board", duplicate=0))

# Add SDK
Import('SIFLI_SDK')
objs.extend(SConscript(os.path.join(SIFLI_SDK, 'SConscript'), variant_dir="sifli_sdk", duplicate=0))

Return('objs')
